{
  "name": "toxy-tech/git-commit-checker",
  "description": "Check coding standard & code syntax with Git pre-commit hook.",
  "type": "package",
  "license": "MIT",
  "keywords": [
    "toxytech",
    "toxytech cms",
    "toxytech platform",
    "toxytech git commit checker"
  ],
  "homepage": "https://toxytech.com",
  "authors": [
    {
      "name": "Adham Elsharkawy",
      "email": "adhamshrqawy@icloud.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "ToxyTech\\GitCommitChecker\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ToxyTech\\GitCommitChecker\\Tests\\": "tests"
    }
  },
  "require": {
    "php": "^8.0|^8.1|^8.2|^8.3",
    "laravel/framework": ">=9.32",
    "laravel/pint": "^1.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0|^10.0|^11.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "ToxyTech\\GitCommitChecker\\Providers\\GitCommitCheckerServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable"
}
